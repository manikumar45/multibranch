pipeline {
    agent any

    stages {
        stage('Master branch') {
            steps {
                sh 'echo "This is master branch"'
            }
        }

        stage('devlopment') {
            steps {
                sh 'echo "This is devlopment branch"'
            }
        }

        stage('Hotfix') {
            steps {
                sh 'echo "This is hotfix branch"'
            }
        }
    }
}
